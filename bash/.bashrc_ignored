export http_proxy="%DOMAIN:%PASSWD@172.18.32.221:8080"
export https_proxy="%DOMAIN:%PASSWD@172.18.32.221:8080"
export ftp_proxy="%DOMAIN:%PASSWD@172.18.32.221:8080"

function on()
{
	export http_proxy="%DOMAIN:%PASSWD@172.18.32.221:8080"
	export https_proxy="%DOMAIN:%PASSWD@172.18.32.221:8080"
	export ftp_proxy="%DOMAIN:%PASSWD@172.18.32.221:8080"
}

function off()
{
	unset http_proxy
	unset https_proxy
	unset ftp_proxy
}

export NO_PROXY="rnd-dockerhub.huawei.com"
