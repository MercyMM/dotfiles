[user]
	name = Ouyang Xiongyi
	email = oywymail@163.com
[color]
	ui = always
[diff]
	tool = vimdiff
[difftool]
	prompt = false
[alias]
	s = status
	d = diff
	ss = show --pretty="" --name-only
[core]
	quotepath = off
[credential]
	helper = cache --timeout=3600
[push]
	default = matching
[include]
	    path = ~/.gitconfig_ignored
