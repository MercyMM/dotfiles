[user]
	name = Ouyang Xiongyi
	email = oywymail@163.com
[color]
	ui = auto
[diff]
	tool = vimdiff
[difftool]
	prompt = false
[alias]
	s = status
	d = diff
	ss = show --stat
	ll = log --stat
[core]
	quotepath = off
	excludesfile = ~/.gitignore_global
[credential]
	helper = cache --timeout=3600
[push]
	default = matching
[include]
	    path = ~/.gitconfig_ignored
